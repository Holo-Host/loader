  \texpect(secondCallResult).toBe(\"override response\");\n\n\n  })\n\n\n  it(\"should be able to call key generation functions\", async () => {\n  \tlet keypair = await hClient.generateReadonlyKey();\n  \tconsole.log(keypair);\n    expect(1).toBe(1);\n  })\n\n\n})\n"},"lineCount":null}},"hash":"030aacec904162151b116ca736426765","cacheData":{"env":{}}}